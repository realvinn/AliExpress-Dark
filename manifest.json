{
    "manifest_version": 3,
    "name": "AliExpress Dark Mode Toggle",
    "version": "1.0",
    "description": "Adds a dark mode toggle to AliExpress.",
    "permissions": [
      "activeTab",
      "scripting"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon128.png",
        "48": "icon128.png",
        "128": "icon128.png"
      }
    },
    "icons": {
      "16": "icon128.png",
      "48": "icon128.png",
      "128": "icon128.png"
    },
    "host_permissions": [
  "*://www.aliexpress.com/*"
],
"content_scripts": [
  {
    "matches": ["*://www.aliexpress.com/*"],
    "js": ["content.js"],
    "css": ["dark-mode.css"]
  }
]

    }
    
  