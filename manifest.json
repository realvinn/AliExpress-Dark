{
    "name": "AliExpress Dark Mode",
    "description": "Toggle a dark theme on AliExpress with a popup button.",
    "version": "1.1",
    "manifest_version": 3,
    "permissions": [
      "storage"             
    ],
    "host_permissions": [
      "*://*.aliexpress.com/*"
    ],
    "content_scripts": [
      {
        "matches": ["*://*.aliexpress.com/*"],
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html" 
    }
  }
  